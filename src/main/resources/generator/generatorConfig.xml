<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- 生成 命令    mybatis-generator:generate  -->
<generatorConfiguration>
    <!-- <properties resource="application.yml"/> -->

    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>



        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/salary?serverTimezone=GMT%2B8"
                        userId="root"
                        password="123456">
        </jdbcConnection>

        <javaModelGenerator targetPackage="com.salary.model" targetProject="src/main/java"/>

        <sqlMapGenerator targetPackage="com.salary.dao.mapper" targetProject="src/main/java"/>

        <javaClientGenerator targetPackage="com.salary.dao" targetProject="src/main/java"
                             type="XMLMAPPER"/>
        <!--自动生成的表 -->
        <table tableName="Place" domainObjectName="Place"  mapperName="PlaceDao" >
            <!-- mysql 配置 -->
            <generatedKey column="id" sqlStatement="Mysql" identity="true"/>
        </table>
        <table tableName="Salary" domainObjectName="Salary"  mapperName="SalaryDao" >
            <!-- mysql 配置 -->
            <generatedKey column="id" sqlStatement="Mysql" identity="true"/>
        </table>
        <table tableName="Employee" domainObjectName="Employee"  mapperName="EmployeeDao" >
            <!-- mysql 配置 -->
            <generatedKey column="id" sqlStatement="Mysql" identity="true"/>
        </table>
<!--        <table tableName="ehr_releases" domainObjectName="Releases"  mapperName="ReleasesDao" >-->
